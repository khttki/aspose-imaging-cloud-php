{
    "description":     "Aspose.Imaging Cloud PHP SDK",
    "name":         "aspose/aspose-imaging-cloud",
    "type":         "library",
    "version":         "20.1",
    "keywords" :     ["aspose", "imaging", "cloud", "rest", "api", "sdk", "image", "bmp", "dicom", "dng", "djvu", "djv", "emf", "gif", "jpg", "jpe", "jpeg", "jpeg2000", "jp2", "jpx", "jpm", "j2k", "odg", "otg", "png", "tiff", "tif", "webp", "wmf", "cdr", "cmx", "pdf", "svg", "reverse", "search"],
    "license":         "MIT",
    "homepage": "https://github.com/aspose-imaging-cloud/aspose-imaging-cloud-php",
    "authors": [
        {
            "name": "Aspose.Imaging Cloud",
            "homepage": "https://github.com/aspose-imaging-cloud"
        }
    ],
    "config": {
        "platform": {
            "php": "5.6.39"
        }
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.3",
        "psr/http-message": "~1.0",
      "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-0": {
            "Aspose": ["lib/"]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Aspose": ["tests/"]
        }
    },
    "archive": {
        "exclude": ["/tests", "/testdata", "phpunit.xml"]
    }
}
